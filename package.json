{
  "name": "watch-hog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.dev -- next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate-dev": "dotenv -e .env.dev -- npx prisma migrate dev"
  },
  "prisma": {
    "seed": "dotenv -e .env.dev -- ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/dev/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
